<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" styleClass="background" stylesheets="@Theme.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coursework.FirstScene">
   <children>
         <ButtonBar layoutX="782.0" layoutY="354.0" prefHeight="40.0" prefWidth="204.0">
         <buttons>
            <Button fx:id="createCollectionButton" mnemonicParsing="false" onAction="#createCollection" prefHeight="33.0" prefWidth="199.0" text="Create a collection" />
         </buttons>
      </ButtonBar>
      <TableView fx:id="tableview" layoutX="35.0" layoutY="28.0" prefHeight="311.0" prefWidth="484.0">
        <columns>
          <TableColumn fx:id="Coins" prefWidth="463.0" text="Search results" />
        </columns>
      </TableView>
      <TableView fx:id="tableview2" layoutX="620.0" layoutY="28.0" prefHeight="311.0" prefWidth="334.0">
        <columns>
          <TableColumn fx:id="Collect" prefWidth="320.0" text="Your collections" />
        </columns>
      </TableView>
      <TextField fx:id="tf" layoutX="175.0" layoutY="357.0" promptText="Search" />
      <Button fx:id="leaveButton" layoutX="35.0" layoutY="544.0" mnemonicParsing="false" onAction="#exit" text="Sign out" />
      <Label fx:id="nickname" layoutX="826.0" layoutY="14.0" text="Without authorization" />
   </children>
</AnchorPane>
